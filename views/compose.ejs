
<%- include("partials/header"); -%>

    <h1> Compose </h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label>Title</label>
            <input class="form-control" type="text" name="postTitle" placeholder="Write Title for your compose">
        </div>
        <div class="form-group">
            <label>Post</label>
            <textarea class="form-control" name="postBody" rows="5" cols="30" placeholder="Write your content here"></textarea>
        </div>
        <button class="btn btn-primary" type="submit" name="button">Publish</button>
    </form>

<%- include("partials/footer"); -%>

